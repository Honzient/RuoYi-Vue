<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.parking.mapper.SysParkingRecordMapper">
    
    <resultMap type="SysParkingRecord" id="SysParkingRecordResult">
        <result property="recordId"    column="record_id"    />
        <result property="plateNumber"    column="plate_number"    />
        <result property="entryTime"    column="entry_time"    />
        <result property="exitTime"    column="exit_time"    />
        <result property="parkingDuration"    column="parking_duration"    />
        <result property="fee"    column="fee"    />
        <result property="status"    column="status"    />
        <result property="entryImage"    column="entry_image"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectSysParkingRecordVo">
        select record_id, plate_number, entry_time, exit_time, parking_duration, fee, status, entry_image, create_by, create_time from sys_parking_record
    </sql>

    <select id="selectSysParkingRecordList" parameterType="SysParkingRecord" resultMap="SysParkingRecordResult">
        <include refid="selectSysParkingRecordVo"/>
        <where>  
            <if test="plateNumber != null  and plateNumber != ''"> and plate_number = #{plateNumber}</if>
            <if test="entryTime != null "> and entry_time = #{entryTime}</if>
            <if test="exitTime != null "> and exit_time = #{exitTime}</if>
            <if test="parkingDuration != null  and parkingDuration != ''"> and parking_duration = #{parkingDuration}</if>
            <if test="fee != null "> and fee = #{fee}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="entryImage != null  and entryImage != ''"> and entry_image = #{entryImage}</if>
        </where>
    </select>
    
    <select id="selectSysParkingRecordByRecordId" parameterType="Long" resultMap="SysParkingRecordResult">
        <include refid="selectSysParkingRecordVo"/>
        where record_id = #{recordId}
    </select>

    <insert id="insertSysParkingRecord" parameterType="SysParkingRecord" useGeneratedKeys="true" keyProperty="recordId">
        insert into sys_parking_record
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="plateNumber != null">plate_number,</if>
            <if test="entryTime != null">entry_time,</if>
            <if test="exitTime != null">exit_time,</if>
            <if test="parkingDuration != null">parking_duration,</if>
            <if test="fee != null">fee,</if>
            <if test="status != null">status,</if>
            <if test="entryImage != null">entry_image,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="plateNumber != null">#{plateNumber},</if>
            <if test="entryTime != null">#{entryTime},</if>
            <if test="exitTime != null">#{exitTime},</if>
            <if test="parkingDuration != null">#{parkingDuration},</if>
            <if test="fee != null">#{fee},</if>
            <if test="status != null">#{status},</if>
            <if test="entryImage != null">#{entryImage},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateSysParkingRecord" parameterType="SysParkingRecord">
        update sys_parking_record
        <trim prefix="SET" suffixOverrides=",">
            <if test="plateNumber != null">plate_number = #{plateNumber},</if>
            <if test="entryTime != null">entry_time = #{entryTime},</if>
            <if test="exitTime != null">exit_time = #{exitTime},</if>
            <if test="parkingDuration != null">parking_duration = #{parkingDuration},</if>
            <if test="fee != null">fee = #{fee},</if>
            <if test="status != null">status = #{status},</if>
            <if test="entryImage != null">entry_image = #{entryImage},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where record_id = #{recordId}
    </update>

    <delete id="deleteSysParkingRecordByRecordId" parameterType="Long">
        delete from sys_parking_record where record_id = #{recordId}
    </delete>

    <delete id="deleteSysParkingRecordByRecordIds" parameterType="String">
        delete from sys_parking_record where record_id in 
        <foreach item="recordId" collection="array" open="(" separator="," close=")">
            #{recordId}
        </foreach>
    </delete>
</mapper>